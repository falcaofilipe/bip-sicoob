{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "projects": {
    "todo-frontend": {
      "projectType": "application",
      "root": "",
      "sourceRoot": "src",
      "architect": {
        "build": {
          "options": {
            "outputPath": "dist/todo-frontend",
            "index": "src/index.html",
            "main": "src/main.ts",
            "styles": ["src/styles.css", "https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/css/bootstrap.min.css"]
          }
        },
        "serve": {
          "options": { "browserTarget": "todo-frontend:build" }
        }
      }
    }
  },
  "defaultProject": "todo-frontend"
}